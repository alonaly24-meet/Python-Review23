def like(dictionary):
	if "likes" in dictionary:
		dictionary[likes]=	dictionary[likes+1]
	return dictionary

def like(dictionary):
	if "likes" in dictionary:
		dictionary[dislikes]=	dictionary[dislikes+1]
	return dictionary

def add_comment(dic, username, comment_text):
	dic["comments"][username]="comment_text"
	return dic

